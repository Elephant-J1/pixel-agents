{
  "name": "pixel-agents-dashboard",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n proxy,ui -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:sim": "SIMULATE=true concurrently -n proxy,ui -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc -b && vite build && tsc -b server/tsconfig.json",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "check-types": "tsc -b --noEmit"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "express": "^5.1.0",
    "ws": "^8.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "tsx": "^4.21.0",
    "vite": "^7.2.4"
  }
}
